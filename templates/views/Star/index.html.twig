<h2>Star</h2>

{% extends "layouts/base.html.twig" %}

{% block app %}

<h2>{{title}}</h2>

<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Size</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        {% for star in stars %}
        <tr>
            <td>{{star.id}}</td>
            <td>{{star.name}}</td>
            <td>{{star.size}}</td>
            <td>{{star.description}}</td>
        </tr>
        {% endfor %}
    </tbody>
</table>

{% endblock %}
